<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

    <servlet>
        <servlet-name>ajaxGetAllUsers</servlet-name>
        <servlet-class>sk.AjaxRequestServlet.GetAllUsersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ajaxGetAllUsers</servlet-name>
        <url-pattern>/ajaxGetAllUsers</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ajaxGetProjectTree</servlet-name>
        <servlet-class>sk.AjaxRequestServlet.GetProjectTreeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ajaxGetProjectTree</servlet-name>
        <url-pattern>/ajaxGetProjectTree</url-pattern>
    </servlet-mapping>

</web-app>
